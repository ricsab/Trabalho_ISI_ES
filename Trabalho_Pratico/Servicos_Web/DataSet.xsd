        <Connections>
          <Connection AppSettingsObjectName="Settings" AppSettingsPropertyName="Trabalho_ISI_ESConnectionString" ConnectionStringObject="" IsAppSettingsProperty="true" Modifier="Assembly" Name="Trabalho_ISI_ESConnectionString (Settings)" ParameterPrefix="@" PropertyReference="ApplicationSettings.Servicos_Web.Properties.Settings.GlobalReference.Default.Trabalho_ISI_ESConnectionString" Provider="System.Data.SqlClient" />
        </Connections>
        <Tables />
        <Sources>
          <DbSource ConnectionRef="Trabalho_ISI_ESConnectionString (Settings)" DbObjectType="Unknown" GenerateShortCommands="true" GeneratorSourceName="ScalarQuery" MethodsParameterType="CLR" Modifier="Public" Name="ScalarQuery" QueryType="Scalar" ScalarCallRetval="System.Boolean, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataBy" UserSourceName="ScalarQuery">
            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT        [Noticias_Nao Residente].[Não ResidenteValidacão], [Noticias_Nao Residente].NoticiasIDNoticias, [Noticias_Nao Residente].[Nao ResidenteID NaoResidente], Administrador.[FuncionarioID Funcionario], 
                         Funcionario.[ID Funcionario], Funcionario.[PessoaID Pessoa], Funcionario.Funçao, Noticias.IDNoticias, Noticias.Descriçao, Noticias.TipoNoticia, Noticias.[FuncionarioID Funcionario] AS Expr1, 
                         [Nao Residente].[PessoaID Pessoa] AS Expr2, [Nao Residente].Validacão, [Nao Residente].[ID NaoResidente], Noticias_Residente.[ResidenteID Residente], Noticias_Residente.ResidenteValidacão, 
                         Noticias_Residente.NoticiasIDNoticias AS Expr3, Pessoa.[ID Pessoa], Pessoa.Nome, Pessoa.Password, Pessoa.Contribuinte, Pessoa.[Numero Cartão Cidadão], Pessoa.[Data Nascimento], Pessoa.[MoradaID Morada], 
                         Morada.[ID Morada], Morada.Rua, Morada.N_Porta, Morada.CP_ID, CP.CP_ID AS Expr4, CP.Localidade, Residente.[ID Residente], Residente.Validacão AS Expr5, Residente.[PessoaID Pessoa] AS Expr6, Servicos.[ID Serviço], 
                         Servicos.Descriçao_Serviço, Servicos.[ResidenteID Residente] AS Expr7, Servicos.[FuncionarioID Funcionario] AS Expr8, Servicos.ResidenteValidacão AS Expr9, Servicos.Data, Servicos.DocsIDDoc, Docs.IDDoc, 
                         Docs.Descriçao AS Expr10, [Servicos Gerais].[ID Serviço Geral], [Servicos Gerais].Descriçao_Serviço AS Expr11, [Servicos Gerais].[FuncionarioID Funcionario] AS Expr12, [Servicos Gerais].[ResidenteID Residente] AS Expr13, 
                         [Servicos Gerais].ResidenteValidacão AS Expr14, [Servicos Gerais].Data AS Expr15, [Servicos Gerais].[Nao ResidenteValidacão], [Servicos Gerais].[Nao ResidenteID NaoResidente] AS Expr16
FROM            [Noticias_Nao Residente] INNER JOIN
                         Administrador INNER JOIN
                         Funcionario ON Administrador.[FuncionarioID Funcionario] = Funcionario.[ID Funcionario] INNER JOIN
                         Noticias ON Funcionario.[ID Funcionario] = Noticias.[FuncionarioID Funcionario] ON [Noticias_Nao Residente].NoticiasIDNoticias = Noticias.IDNoticias INNER JOIN
                         [Nao Residente] ON [Noticias_Nao Residente].[Não ResidenteValidacão] = [Nao Residente].Validacão AND [Noticias_Nao Residente].[Nao ResidenteID NaoResidente] = [Nao Residente].[ID NaoResidente] INNER JOIN
                         Noticias_Residente ON Noticias.IDNoticias = Noticias_Residente.NoticiasIDNoticias INNER JOIN
                         Pessoa ON Funcionario.[PessoaID Pessoa] = Pessoa.[ID Pessoa] AND [Nao Residente].[PessoaID Pessoa] = Pessoa.[ID Pessoa] INNER JOIN
                         Morada INNER JOIN
                         CP ON Morada.CP_ID = CP.CP_ID ON Pessoa.[MoradaID Morada] = Morada.[ID Morada] INNER JOIN
                         Residente ON Noticias_Residente.[ResidenteID Residente] = Residente.[ID Residente] AND Noticias_Residente.ResidenteValidacão = Residente.Validacão AND Pessoa.[ID Pessoa] = Residente.[PessoaID Pessoa] INNER JOIN
                         Servicos ON Funcionario.[ID Funcionario] = Servicos.[FuncionarioID Funcionario] AND Residente.[ID Residente] = Servicos.[ResidenteID Residente] AND Residente.Validacão = Servicos.ResidenteValidacão INNER JOIN
                         Docs ON Servicos.DocsIDDoc = Docs.IDDoc INNER JOIN
                         [Servicos Gerais] ON Funcionario.[ID Funcionario] = [Servicos Gerais].[FuncionarioID Funcionario] AND [Nao Residente].Validacão = [Servicos Gerais].[Nao ResidenteValidacão] AND 
                         [Nao Residente].[ID NaoResidente] = [Servicos Gerais].[Nao ResidenteID NaoResidente] AND Residente.[ID Residente] = [Servicos Gerais].[ResidenteID Residente] AND 
                         Residente.Validacão = [Servicos Gerais].ResidenteValidacão</CommandText>
                <Parameters />
              </DbCommand>
            </SelectCommand>
          </DbSource>
        </Sources>
      </DataSource>
